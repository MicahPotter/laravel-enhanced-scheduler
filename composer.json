{
  "name": "micahpotter/laravel-enhanced-scheduler",
  "description": "Enhanced logging and distributed capabilities for Laravel's scheduler.",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Micah Potter",
      "email": "johnmicah.potter@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": ">=5.2",
    "illuminate/console": ">=5.2",
    "illuminate/database": ">=5.2",
    "symfony/process": ">=2.6",
    "symfony/var-dumper": ">=2.6"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Myriad\\Illuminate\\Console\\Scheduling\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/EnhancedSchedulerTestCase.php"
    ]
  }
}
